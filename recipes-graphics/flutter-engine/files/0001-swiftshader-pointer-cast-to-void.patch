From 84d51cdf981b719ebacee7096fbb1678c8bf8c3c Mon Sep 17 00:00:00 2001
From: Joel Winarske <joel.winarske@gmail.com>
Date: Tue, 8 Jul 2025 16:54:36 -0700
Subject: [PATCH] swiftshader pointer cast to void*

Upstream-Status: Inappropriate
---
 engine/src/flutter/third_party/swiftshader/src/Device/Memset.hpp | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/engine/src/flutter/third_party/swiftshader/src/Device/Memset.hpp b/engine/src/flutter/third_party/swiftshader/src/Device/Memset.hpp
index 91c7e4d73..7cf7a7658 100644
--- a/engine/src/flutter/third_party/swiftshader/src/Device/Memset.hpp
+++ b/engine/src/flutter/third_party/swiftshader/src/Device/Memset.hpp
@@ -39,7 +39,7 @@ struct Memset
 	{
 		static_assert(std::is_base_of<Memset<T>, T>::value, "Memset<T> must only clear the memory of a type of which it is a base class");
 		static_assert(!std::is_polymorphic<T>::value, "Memset<T> must not be used with classes that have virtual functions");
-		::memset(object, 0, sizeof(T));
+		::memset(static_cast<void*>(object), 0, sizeof(T));
 	}
 
 	// Don't rely on the implicitly declared copy constructor and copy assignment operator.
