From 5047485921c7499987b7dd0f66902803cc7c3c9b Mon Sep 17 00:00:00 2001
From: Joel Winarske <joel.winarske@gmail.com>
Date: Tue, 8 Jul 2025 15:48:53 -0700
Subject: [PATCH] skia memcpy void* cast

Upstream-Status: Inappropriate
---
 engine/src/flutter/third_party/skia/src/gpu/tessellate/MiddleOutPolygonTriangulator.h | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/engine/src/flutter/third_party/skia/src/gpu/tessellate/MiddleOutPolygonTriangulator.h b/engine/src/flutter/third_party/skia/src/gpu/tessellate/MiddleOutPolygonTriangulator.h
index bc7895b3ac..cdff47976c 100644
--- a/engine/src/flutter/third_party/skia/src/gpu/tessellate/MiddleOutPolygonTriangulator.h
+++ b/engine/src/flutter/third_party/skia/src/gpu/tessellate/MiddleOutPolygonTriangulator.h
@@ -90,7 +90,7 @@ public:
         }
 
         PoppedTriangleStack(PoppedTriangleStack&& that) {
-            memcpy(this, &that, sizeof(*this));
+            memcpy(static_cast<void*>(this), &that, sizeof(*this));
             that.fMiddleOut = nullptr;  // Don't do a stack update during our destructor.
         }
 
